package com.rxtrack.model;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class Lookups {
	private static Lookups instance;
	
	private Map<String, List<String>> lookupLists = new HashMap<String, List<String>>();
	
	public static synchronized Lookups getInstance() {
		if (instance != null) {
			return instance;
		}
		instance = new Lookups();
		return instance;
	}
	
	public List<String> getLookup(String key){
		List<String> list = lookupLists.get(key);
		if (list==null){
			list = new ArrayList<String>();
			lookupLists.put(key, list);
		}
		return list;
	}
}
